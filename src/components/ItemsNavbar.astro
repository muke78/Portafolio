---
import { dataListNavbar } from '../utils/dataNavbar.astro';
import { dataListNavbarEN } from '../utils/dataNavbarEN.astro';

const { currentLocale } = Astro;
let dataChange;
dataChange = currentLocale === 'es' ? dataListNavbar : dataListNavbarEN;
---

{
  dataChange.map((list) => (
    <li>
      <a href={list.to}> {list.label} </a>
    </li>
  ))
}
